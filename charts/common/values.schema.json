                        },
                        "timeoutSeconds": {
                            "$id": "#/properties/probes/properties/liveness/properties/timeoutSeconds",
                            "default": 10,
                            "description": "An explanation about the purpose of this instance.",
                            "title": "Timeout seconds for liveness probe",
                            "type": "integer"
                        }
                    }
