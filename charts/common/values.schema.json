                        "initialDelaySeconds": {
                            "$id": "#/properties/probes/properties/readiness/properties/initialDelaySeconds",
                            "default": 30,
                            "description": "An explanation about the purpose of this instance.",
                            "title": "Initial delay seconds for readiness probe",
