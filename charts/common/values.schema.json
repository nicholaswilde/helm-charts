                        "timeoutSeconds": {
                            "$id": "#/properties/probes/properties/readiness/properties/timeoutSeconds",
                            "default": 0,
                            "description": "An explanation about the purpose of this instance.",
                            "title": "Timeout seconds for readiness probe",
                            "type": "integer"
                        }
                    }
